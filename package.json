{
  "type": "module",
  "name": "@hazae41/ripemd160",
  "version": "0.0.0",
  "description": "RIPEMD-160 adapter for WebAssembly and JS implementations",
  "homepage": "https://github.com/hazae41/ripemd160",
  "repository": "github:hazae41/ripemd160",
  "author": "hazae41",
  "license": "MIT",
  "main": "./dist/cjs/index.cjs",
  "module": "./dist/esm/index.mjs",
  "types": "./dist/types/index.d.ts",
  "sideEffects": false,
  "files": [
    "./dist/esm",
    "./dist/cjs",
    "./dist/types"
  ],
  "scripts": {
    "build": "rimraf dist && rollup -c",
    "test": "node ./dist/test/index.test.mjs",
    "prepare": "npm run build"
  },
  "devDependencies": {
    "@hazae41/cursor": "^1.1.26",
    "@hazae41/morax": "^1.1.2",
    "@hazae41/phobos": "^1.0.10",
    "@noble/hashes": "^1.3.2",
    "@rollup/plugin-typescript": "^11.1.3",
    "@types/node": "^20.6.0",
    "rimraf": "^5.0.1",
    "rollup": "^3.29.1",
    "rollup-plugin-dts": "^6.0.2",
    "rollup-plugin-node-externals": "^6.1.1",
    "typescript": "^5.2.2"
  },
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/esm/index.mjs",
      "require": "./dist/cjs/index.cjs"
    }
  },
  "keywords": [
    "ripemd160",
    "ripemd-160",
    "crypto",
    "hashing",
    "cryptography",
    "adapter",
    "typescript",
    "esmodules"
  ],
  "dependencies": {
    "@hazae41/option": "^1.0.21",
    "@hazae41/result": "^1.1.4"
  }
}
